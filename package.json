{
  "name": "ropac",
  "version": "1.0.0",
  "description": "Role-based permission and access control library for React",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaxtonTerryDev/ropac.git"
  },
  "author": "PaxtonTerryDev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PaxtonTerryDev/ropac/issues"
  },
  "homepage": "https://github.com/PaxtonTerryDev/ropac#readme",
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@types/react": "^19.0.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  }
}
